<Defs>
  <!-- Prosthetic finger -->
  <RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
    <defName>InstallSimpleFinger</defName>
    <label>install simple finger</label>
    <description>Install a simple finger.</description>
    <descriptionHyperlinks>
      <ThingDef>SimpleFinger</ThingDef>
    </descriptionHyperlinks>
    <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
    <jobString>Installing simple finger.</jobString>
    <workAmount>250</workAmount>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Medicine</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>SimpleFinger</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Medicine</li>
      </categories>
      <thingDefs>
        <li>SimpleFinger</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Finger</li>
    </appliedOnFixedBodyParts>
    <addsHediff>SimpleFinger</addsHediff>
  </RecipeDef>
